<div class="row">     
    <div class="col-md-12">           
        <h2>People</h2>    
        <hr/>
        <div class="table-responsive">
            <table class="table">
                <thead>
                    <tr>
                        <th>Person&nbsp;ID</th>
                        <th>Full&nbsp;Name</th>
                        <th>Phone Number</th>
                        <th>Address</th>
                        <th>Car Vin</th>
                    </tr>
                </thead>
                <tbody>
                {{#each people}}
                <tr>
                    <td>{{id}}</td>
                    <td>{{first_name}} {{last_name}}</td>
                    <td>{{phone}}</td>
                    <td>{{address}} {{city}}</td>
                    <td>{{vin}}</td>
                </tr>
                {{/each}}
                </tbody>
            </table> 
        </div>  
        {{#unless people.contact}}  //not sure
            <div class="col-md-12 text-center">             
                <strong>{{message}}</strong> 
            </div> 
        {{/unless}}   
        
          
            
        
    </div> 
</div> 